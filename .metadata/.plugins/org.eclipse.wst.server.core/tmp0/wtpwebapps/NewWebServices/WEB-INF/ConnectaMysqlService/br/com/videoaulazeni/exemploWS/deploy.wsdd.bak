<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ConnectaMysqlService WSDL service -->

  <service name="ConnectaMysql" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://exemploWS.videoaulazeni.com.br"/>
      <parameter name="wsdlServiceElement" value="ConnectaMysqlService"/>
      <parameter name="schemaQualified" value="http://exemploWS.videoaulazeni.com.br"/>
      <parameter name="wsdlServicePort" value="ConnectaMysql"/>
      <parameter name="className" value="br.com.videoaulazeni.exemploWS.ConnectaMysqlSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ConnectaMysql"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="obterConexao" qname="operNS:obterConexao" xmlns:operNS="http://exemploWS.videoaulazeni.com.br" returnQName="retNS:obterConexaoReturn" xmlns:retNS="http://exemploWS.videoaulazeni.com.br" returnType="rtns:anyType" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="obterConexao"/>

  </service>
</deployment>
